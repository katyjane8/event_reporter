<html>
<head>
<style>
 table {
   font-family: arial, sans-serif;
   border-collapse: collapse;
   width: 100%;
}

td, th {
   border: 1px solid #dddddd;
   text-align: left;
   padding: 8px;
}
</style>
</head>
<table>
   <thead>
       <th>First Name</th>
       <th>Last Name</th>
       <th>Email Address</th>
       <th>Home Phone</th>
       <th>Street</th>
       <th>City</th>
       <th>State</th>
       <th>Zipcode</th>
   </thead>
     <% @list.each do |attendee| %>
   <tr>
       <td><%= attendee.first_name%></td>
       <td><%= attendee.last_name%></td>
       <td><%= attendee.email_address%></td>
       <td><%= attendee.home_phone%></td>
       <td><%= attendee.street%></td>
       <td><%= attendee.city%></td>
       <td><%= attendee.state%></td>
       <td><%= attendee.zipcode%></td>
   </tr>
   <% end %>
</table>
</html>
